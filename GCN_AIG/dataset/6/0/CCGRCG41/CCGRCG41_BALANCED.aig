module CCGRCG41 ( 
    x0, x1, x2,
    f1, f2, f3  );
  input  x0, x1, x2;
  output f1, f2, f3;
  wire new_n8_, new_n9_, new_n11_, new_n12_;
  assign f1 = x0 | x1;
  assign new_n8_ = ~x0 & x2;
  assign new_n9_ = x0 & ~x2;
  assign f2 = ~new_n8_ & ~new_n9_;
  assign new_n11_ = ~x1 & x2;
  assign new_n12_ = x1 & ~x2;
  assign f3 = ~new_n11_ & ~new_n12_;
endmodule


