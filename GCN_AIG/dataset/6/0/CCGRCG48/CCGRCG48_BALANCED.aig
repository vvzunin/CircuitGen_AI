module CCGRCG48 ( 
    x0, x1, x2,
    f1, f2, f3, f4, f5, f6, f7  );
  input  x0, x1, x2;
  output f1, f2, f3, f4, f5, f6, f7;
  assign f2 = ~x0 & ~x2;
  assign f3 = x1 & x2;
  assign f1 = ~x0;
  assign f6 = ~x1;
  assign f4 = f2;
  assign f5 = f1;
  assign f7 = f3;
endmodule


